{
    "name": "jgustavo99/gerencianet-laravel5",
    "description": "Integração da Gerencianet SDK para Laravel 8",
    "keywords": ["Gerencianet", "SDK", "Laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "João Gustavo Balestrin dos Santos",
            "email": "joaogustavo.b@hotmail.com"
        }
    ],
    "require": {
        "gerencianet/gerencianet-sdk-php": "4.*",
        "illuminate/support": "6.*|7.*|8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "orchestra/testbench": "^6.18"
    },
    "autoload" : {
        "psr-4" : {
            "Jgustavo99\\Gerencianet\\": "src/Jgustavo99/Gerencianet/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Jgustavo99\\Gerencianet\\Tests\\": "tests/Jgustavo99/Gerencianet/"
        }
    }

}
